		int nota=0;
		
		System.out.println("BIENVENIDO AL PROGRAMA:");
		System.out.println("-----------------------------------\n");
		do {
			
			System.out.println("Indique la nota del 1-10, pulse 0 para salir:");
			aux=sc.nextLine();
			nota=Integer.parseInt(aux);
			switch(nota){
			
			}
			
		}while(nota!=0);
		
		System.out.println("Gracias por su atención");
		
	}

}
